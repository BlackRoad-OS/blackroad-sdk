{
  "name": "@blackroad/sdk",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for the BlackRoad API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "blackroad",
    "api",
    "sdk",
    "agents",
    "ai",
    "infrastructure"
  ],
  "author": "BlackRoad OS, Inc.",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/BlackRoad-OS/blackroad-sdk-js.git"
  },
  "homepage": "https://docs.blackroad.io/sdk/javascript",
  "bugs": {
    "url": "https://github.com/BlackRoad-OS/blackroad-sdk-js/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
