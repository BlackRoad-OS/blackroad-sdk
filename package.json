{
  "name": "@blackroad/sdk",
  "version": "1.0.0",
  "description": "Official BlackRoad SDK - Unified client for all BlackRoad APIs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "blackroad",
    "sdk",
    "api",
    "graphql",
    "webhooks",
    "ai",
    "infrastructure"
  ],
  "author": "BlackRoad OS, Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BlackRoad-OS/blackroad-sdk"
  },
  "homepage": "https://blackroad.io",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
